{
  "name": "motia",
  "description": "Build production-grade backends with a single primitive. APIs, background jobs, Queues, Workflows, and AI agents - unified in one system with built-in State management, Streaming, and Observability.",
  "version": "0.14.0-beta.164",
  "license": "Elastic-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/motiadev/motia.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./cli": {
      "development": "./src/cli.ts",
      "default": "./dist/cli.mjs"
    },
    "./workbench": {
      "development": "./src/workbench.ts",
      "default": "./dist/workbench.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "motia": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "move:templates": "sh scripts/move-templates.sh",
    "move:python": "sh scripts/move-python.sh",
    "move:dot-files": "sh scripts/move-dot-files.sh",
    "build": "tsdown",
    "postbuild": "sh scripts/post-build.sh",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "biome check .",
    "lint:plugins": "eslint --config ../../eslint.config.js"
  },
  "dependencies": {
    "@amplitude/analytics-node": "^1.3.8",
    "@motiadev/adapter-bullmq-events": "workspace:*",
    "@motiadev/adapter-redis-cron": "workspace:*",
    "@motiadev/adapter-redis-state": "workspace:*",
    "@motiadev/adapter-redis-streams": "workspace:*",
    "@motiadev/core": "workspace:*",
    "@motiadev/stream-client-node": "workspace:*",
    "@motiadev/workbench": "workspace:*",
    "antlr4ts": "0.5.0-alpha.4",
    "archiver": "^7.0.1",
    "axios": "^1.8.2",
    "chokidar": "^4.0.3",
    "picocolors": "^1.1.1",
    "commander": "^13.0.0",
    "cron": "^4.3.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "express": "^4.21.2",
    "tsx": "^4.19.0",
    "glob": "^11.0.1",
    "inquirer": "^8.2.5",
    "node-cron": "^3.0.3",
    "python-ast": "^0.1.0",
    "redis": "^5.9.0",
    "redis-memory-server": "^0.14.0",
    "table": "^6.9.0",
    "ts-node": "^10.9.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@amplitude/analytics-types": "^2.9.2",
    "@types/archiver": "^6.0.3",
    "@types/express": "^5.0.3",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node-cron": "^3.0.11",
    "jest": "^29.7.0",
    "openapi-types": "^12.1.3",
    "ts-jest": "^29.2.5",
    "tsdown": "^0.16.6",
    "typescript": "^5.7.2",
    "typescript-transform-paths": "^3.5.3",
    "zod": "^4.1.13"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./cli": "./dist/cli.mjs",
      "./workbench": "./dist/workbench.mjs",
      "./package.json": "./package.json"
    }
  }
}
